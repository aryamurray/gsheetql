{
  "name": "gsheetql",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "module": "index.ts",
  "scripts": {
    "build": "rm -rf build && tsc && webpack",
    "push": "npm run build && clasp push -f",
    "deploy": "npm run push && clasp deploy -i $(clasp deployments | grep '@HEAD' | awk '{print $2}')",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:critical": "eslint src --quiet --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "login": "clasp login"
  },
  "devDependencies": {
    "@google/clasp": "3.1.1",
    "@types/google-apps-script": "2.0.7",
    "eslint-plugin-googleappsscript": "^1.0.5",
    "gas-webpack-plugin": "^2.6.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
