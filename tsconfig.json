{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "outDir": "build",
    "lib": ["ESNext"],
    "types": ["google-apps-script"],
    "skipLibCheck": true
  },
  "include": ["src"],
  "files": ["src/index.ts"]
}
